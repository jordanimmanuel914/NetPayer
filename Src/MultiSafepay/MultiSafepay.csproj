<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.8.3</Version>
    <PackageId>MultiSafepay</PackageId>
    <Product>MultiSafepay Payments</Product>
    <Authors>Jorge Sariego</Authors>
    <PackageProjectUrl>https://github.com/MultiSafepay/.Net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MultiSafepay/.Net</RepositoryUrl>
    <Description>A .NET client for the MultiSafepay payments API. Provides support for all payment methods, Pay After Delivery and Fast Checkout.</Description>
    <PackageIcon>favicon.png</PackageIcon>
    <PackageIconUrl />
    <PackageReleaseNotes>Add IdealQr missing parameters [DNTMINT-27]
min_amount
in addition:
max_amount
allow_multiple
allow_change_amount</PackageReleaseNotes>
    <Company>MultiSafepay</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\favicon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
</Project>
